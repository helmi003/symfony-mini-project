<!DOCTYPE html>
<html>
<head>
<script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
<meta charset="utf-8">
<title>
{% block title %}
account
{% endblock %}
</title>
<link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css ">
</head>
<body>
{% block body %}
<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
  <div class="collapse navbar-collapse" id="navbarNav">
    <ul class="navbar-nav">
      <li class="nav-item">
        <a class="nav-link" href="../index">Home</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="../clubs">clubs</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="../membership">Member Ship</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="../about">About</a>
      </li>
      <li class="nav-item active">
        {% if app.user %}
          <a class="nav-link" href="../account/{{ app.user.id }}">Acount</a>
        {% else %}
          <a class="nav-link" href="../login">Acount</a>
        {% endif %}
      </li>
    </ul>
  </div>
</nav>
<body>
<div class="mb-3">
    You are logged in as {{ app.user.lastname }}, <a href="{{ path('app_logout') }}">Logout</a>
</div>
<table class="table table-hover table-striped" border="1">
<thead>
<tr>
<th>clubs joined</th>
</tr>
</thead>
<tbody>
{% for myclub in myClubs %}
<tr>
<td>{{myclub.clubname}}     <a class="btn btn-danger" href="{{path('inscriseance_supprimer', {'id':seance.id})}}">Supprimer</a></td>
</tr>
{% else %}
<tr>
<td >Aucun club joined</td>
</tr>
{% endfor %}
</tbody>
</table>
<script>
$(".btn-danger").click(function(){
return confirm("wanna left ?");
})
</script>
</body>
{% endblock %}